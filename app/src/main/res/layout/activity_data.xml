<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="imaginatio.databasetesting.data">



    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="miRNA"
        android:textColor="#000"
        android:id="@+id/mirna"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/mirna_data"
        android:layout_below="@+id/mirna"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Mechanism"
        android:textColor="#000"
        android:id="@+id/mechanism"
        android:layout_below="@+id/mirna_data"
        android:layout_alignParentStart="true"
        android:layout_marginTop="44dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/mechanism_dats"
        android:layout_below="@+id/mechanism"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Disease"
        android:textColor="#000"
        android:id="@+id/disease"
        android:layout_below="@+id/mechanism_dats"
        android:layout_alignParentStart="true"
        android:layout_marginTop="44dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/disease_data"
        android:layout_below="@+id/disease"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="pathway"
        android:textColor="#000"
        android:paddingTop="50sp"
        android:layout_below="@+id/disease_data"
        android:id="@+id/pathway"
        android:layout_centerVertical="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/pathway_data"
        android:layout_below="@+id/pathway"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Etilogy"
        android:textColor="#000"
        android:id="@+id/Etilogy"
        android:layout_marginTop="72dp"
        android:layout_below="@+id/pathway_data"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="new tes"
        android:id="@+id/etilogy_data"
        android:layout_below="@+id/Etilogy"
        android:layout_alignParentStart="true" />

</RelativeLayout>
